<latemp_html_header />
<latemp_html_head_tag />

<define-tag perl_begin_news endtag="required" whitespace="delete">
{#PERL_BEGIN_NEWS#:%body:##}
</define-tag>

<define-tag perl_begin_sidebar endtag="required" whitespace="delete">
